{
  "name": "@haseebanwar/create-react-package",
  "version": "0.1.28",
  "description": "Zero config CLI for creating react packages",
  "bin": {
    "create-react-package": "lib/index.js"
  },
  "files": [
    "lib",
    "templates"
  ],
  "keywords": [
    "react",
    "cli",
    "package",
    "library",
    "rollup"
  ],
  "author": "Haseeb Anwar (https://haseebanwar.net/)",
  "license": "MIT",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "start": "npm run clean && babel src -d lib --root-mode upward --watch",
    "build": "npm run clean && babel src -d lib --root-mode upward",
    "prepublishOnly": "npm run build",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.3.5",
    "validate-npm-package-name": "^3.0.0"
  },
  "gitHead": "68f174019962a73933378ab07b69fde97382ccc2"
}
